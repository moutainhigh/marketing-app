<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="0.67dp" android:color="#2E9BDD" />
            <solid android:color="@color/rt_retail_color_2E9BDD" />
            <corners android:bottomRightRadius="6dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="0.67dp" android:color="@color/retail_gray" />
            <solid android:color="@color/retail_gray" />
            <corners android:bottomRightRadius="6dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <stroke android:width="0.67dp" android:color="@color/rt_retail_color_32ADF6" />
            <solid android:color="@color/rt_retail_color_32ADF6" />
            <corners android:bottomRightRadius="6dp" />
        </shape>
    </item>

</selector>