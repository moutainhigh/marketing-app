<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#9903A9F5" />
            <corners android:radius="2.67dp" />
            <stroke android:color="#81D3F8" android:width="0.67dp" />
        </shape>
    </item>

    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="#F7F7F7" />
            <corners android:radius="2.67dp" />
            <stroke android:color="@color/color_999999" android:width="0.67dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#E9F7FF" />
            <corners android:radius="2.67dp" />
            <stroke android:color="#32ADF6" android:width="0.67dp" />
        </shape>
    </item>
</selector>