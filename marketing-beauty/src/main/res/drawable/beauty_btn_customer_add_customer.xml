<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/beauty_btn_skroke_width" android:color="#FFFFFF" />
            <solid android:color="#99494949" />
            <corners android:radius="@dimen/beauty_btn_add_customer_store_radius" />
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/beauty_btn_skroke_width" android:color="#FFFFFF" />
            <corners android:radius="@dimen/beauty_btn_add_customer_store_radius" />
        </shape>
    </item>
</selector>