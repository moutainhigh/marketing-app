<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#05000000" />
            <stroke android:width="@dimen/beauty_btn_skroke_width" android:color="@color/beauty_bg_deep_gray" />
            <corners android:radius="20dp" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#EE5E1F" />
            <stroke android:width="@dimen/beauty_btn_skroke_width" android:color="#EE5E1F" />
            <corners android:radius="20dp" />
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="#EE5E1F" />
            <stroke android:width="@dimen/beauty_btn_skroke_width" android:color="#EE5E1F" />
            <corners android:radius="20dp" />
        </shape>
    </item>
</selector>