        <shape>
            <solid android:color="@color/beauty_color_btn_red_pressed" />
            <corners android:bottomRightRadius="8dp" android:bottomLeftRadius="8dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/beauty_color_btn_red_nomal" />
            <corners android:bottomRightRadius="8dp" android:bottomLeftRadius="8dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/beauty_color_btn_red_nomal" />
            <corners android:bottomRightRadius="8dp" android:bottomLeftRadius="8dp" />
        </shape>
    </item>
</selector>