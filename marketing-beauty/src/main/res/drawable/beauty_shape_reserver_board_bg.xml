<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/beauty_bg_white" />
            <stroke
                android:width="@dimen/beauty_stroke_width"
                android:color="@color/beauty_bg_shallow_gray" />
        </shape>
    </item>
    <item
        android:left="@dimen/beauty_stroke_width"
        android:bottom="@dimen/beauty_stroke_width"
        android:top="@dimen/beauty_stroke_width">
        <shape android:shape="rectangle">
            <solid android:color="@color/beauty_bg_white" />
        </shape>
    </item>

</layer-list>