<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="group_create_info_item_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
    </style>

    <style name="group_create_info_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/text_12</item>
    </style>

    <style name="group_create_info_text_red">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_FF5363</item>
        <item name="android:textSize">@dimen/text_12</item>
    </style>

    <style name="group_create_info_edittext_only_text">
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/text_13</item>
        <item name="android:textColorHint">@color/color_bcbcbc</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
    </style>

    <style name="group_create_info_edittext">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/text_13</item>
        <item name="android:textColorHint">@color/color_bcbcbc</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/customer_search_background</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
    </style>

    <style name="group_create_info_sex">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:scaleType">centerInside</item>
    </style>

    <style name="group_create_info_selector_model">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
    </style>

    <style name="group_create_info_selector_model_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/text_13</item>
    </style>

    <style name="group_order_button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/shape_group_bottom_button</item>
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:textColor">@color/color_32ADF6</item>
    </style>

    <style name="group_space_layout">
        <item name="android:layout_marginLeft">23dp</item>
        <item name="android:layout_marginRight">23dp</item>
    </style>

    <style name="group_space_layout_root">
        <item name="android:layout_marginLeft">23dp</item>
    </style>

    <style name="group_title_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="group_date_root_layout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="group_date_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="group_date_week_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_15</item>
    </style>

    <style name="group_date_time_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_12</item>
    </style>

    <style name="group_date_red_point">
        <item name="android:layout_width">5dp</item>
        <item name="android:layout_height">5dp</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/booking_date_prompt</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="group_order_item_button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/uion_sale_and_active_button_normal</item>
        <item name="android:textColor">@color/customer_sale_and_active_normal</item>
        <item name="android:textSize">@dimen/text_12</item>
    </style>

    <style name="group_order_item_button_rootlayout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="group_order_item_button_linerlayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="group_order_item_button_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>


    <style name="group_order_item_button_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/customer_sale_and_active_normal</item>
        <item name="android:textSize">@dimen/text_13</item>
    </style>

    <style name="group_order_item_h_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="group_order_item_v_layout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">31dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="group_order_item_title_l">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
