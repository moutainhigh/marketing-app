<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="bakery_book_detail_ll_module">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_marginTop">21dp</item>
    </style>

    <style name="bakery_book_detail_tv_title">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="bakery_book_detail_ll_item">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">8dp</item>
    </style>

    <style name="bakery_book_detail_tv_item_prefix">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/color_999999</item>
    </style>

    <style name="bakery_book_detail_tv_item">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#444444</item>
    </style>

    <style name="BakeryText">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:contentDescription">belong bakery</item>
    </style>

    <style name="BakeryText.Title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">22sp</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="BakeryText.Button">
        <item name="android:layout_height">52dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#AAAAAA</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="BakeryText.Button.Style1">
        <item name="android:textColor">#32ADF6</item>
    </style>

    <style name="BakeryText.Property">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#999999</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="BakeryText.List">
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="BakeryText.Name">
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="BakeryText.Name.Gray">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#999999</item>
    </style>

    <style name="BakeryText.Phone">
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="BakeryText.Date">
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="BakeryText.Time">
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="BakeryText.Time.Gray">
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#999999</item>
    </style>

    <style name="BakeryLayout">

    </style>

    <style name="BakeryLayout.Title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
        <item name="android:background">@drawable/cashier_bg_title_bar</item>
    </style>

    <style name="BakeryLayout.Content">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/booking_list_background_color</item>
    </style>

    <style name="BakeryLayout.Content.Corner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/bakery_manager_detail_contant_bg</item>
        <item name="android:layout_marginLeft">11dp</item>
        <item name="android:layout_marginTop">12dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginBottom">12dp</item>
    </style>

    <style name="BakeryImageButton">
        <item name="android:contentDescription">Bakery ImageButton</item>
    </style>

    <style name="BakeryImageButton.Icon">
        <item name="android:background">@drawable/dinner_table_info_button_selector</item>
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:scaleType">centerInside</item>
    </style>

    <style name="BakeryLine">

    </style>

    <style name="BakeryLine.Title">
        <item name="android:layout_width">2dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/line</item>
    </style>

    <style name="BakeryLine.Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">#E2E4EA</item>
    </style>
</resources>