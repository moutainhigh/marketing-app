<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="layout"></style>

    <style name="layout.m">
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="layout.w">
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="layout.m.w">
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="layout.m.m">
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="layout.w.m">
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="layout.w.w">
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="order_panel_button_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/orderdish_select_dish_item_bg</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/text_blue</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:drawablePadding">3dp</item>
    </style>

    <style name="custom_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/verify_perssion_title</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="view_page_title">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/verify_perssion_title</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="tv_ticket_column">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/text_12</item>
        <item name="android:textColor">#9ba5ab</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>

    </style>
</resources>