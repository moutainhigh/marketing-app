<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->

    <!-- Application theme. -->
    <style name="AppTheme" parent="@android:style/Theme.Light.NoTitleBar.Fullscreen">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <item name="android:backgroundDimEnabled">true</item>
        　　
    </style>
    <!-- .Dialog -->
    <style name="common_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="common_no_dark_dialog" parent="common_dialog">
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="common_transparent_dialog" parent="common_dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="common_transparent_dialog_style1" parent="common_dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
    </style>

    <style name="login_theme" parent="dialog">
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFullscreen">false</item>
    </style>

    <style name="wxalert">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#444140</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <!--<style name="wxButton">
        <item name="android:textSize">24sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:textColor">@drawable/wx_pay_but_text</item>
        <item name="android:background">@drawable/wx_but_onclick</item>
    </style>-->

    <style name="goodsTotal">
        <item name="android:textColor">@color/goods_total_text_color</item>
        <item name="android:textSize">@dimen/text_14</item>
    </style>

    <style name="dishMemoStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/orderdish_text_black</item>
    </style>

    <style name="dishOrderTextStyle">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/orderdish_text_black</item>
    </style>

    <style name="Transparent" parent="@android:style/Theme.NoTitleBar.Fullscreen">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <item name="android:backgroundDimEnabled">true</item>
        　　
    </style>

    <style name="custom_alert_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
    </style>

    <style name="CheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_style</item>
    </style>

    <style name="date_dialog" parent="@style/custom_alert_dialog">
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>


    <style name="dinner_table_info_waiterwindow_style">
        <item name="android:windowEnterAnimation">@anim/dinner_table_info_waiterwindow_show</item>
        <item name="android:windowExitAnimation">@anim/dinner_table_info_waiterwindow_dismiss</item>
    </style>

    <!-- 第二屏显示 -->
    <style name="display_txt_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">34sp</item>
        <item name="android:textColor">@color/display_txt_color</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="dispaly_small_txt_style" parent="@style/display_txt_style">
        <item name="android:textSize">40sp</item>
    </style>

    <style name="dispaly_txt_small_style" parent="@style/display_txt_style">
        <item name="android:textSize">30sp</item>
    </style>

    <style name="dispaly_txt_middle_style" parent="@style/display_txt_style">
        <item name="android:textSize">45sp</item>
    </style>

    <style name="display_txt_large_style" parent="@style/display_txt_style">
        <item name="android:textSize">50sp</item>
    </style>

    <style name="display_txt_orange_style" parent="@style/display_txt_large_style">
        <item name="android:textColor">@color/display_txt_orange</item>
    </style>

    <!-- fade in or out animation -->

    <!--标题文本-->
    <style name="TitleTextStyle">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/text_22</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!--底部按钮-->
    <style name="BottomButtonStyle">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/btn_blue_selector</item>
        <item name="android:gravity">center</item>
    </style>

    <!--订单中心底部按钮-->
    <style name="OrderCenterBottomButtonStyle" parent="BottomButtonStyle">
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/beauty_btn_main_selector</item>
        <item name="android:maxLines">2</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:textSize">@dimen/text_14</item>
    </style>

    <!--正快餐左侧分栏项-->
    <style name="LeftItemStyle">
        <item name="android:textColor">@color/dinner_dishname_color</item>
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">75dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:drawablePadding">16dp</item>
        <item name="android:paddingLeft">16dp</item>
    </style>

    <!--属性动作栏位按钮风格-->
    <style name="PropertyActionButtonStyle">
        <item name="android:textSize">@dimen/text_16</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/property_action_button_height</item>
        <item name="android:gravity">center</item>
    </style>

    <!--菜品属性动作栏位-->
    <style name="DishPropertyActionButtonStyle" parent="PropertyActionButtonStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/dish_property_action_button_text_selector</item>
        <item name="android:layout_marginTop">@dimen/dish_property_action_button_margin_top</item>
        <item name="android:background">@drawable/dish_property_action_button_bg</item>
        <item name="android:layout_marginLeft">13dp</item>
        <item name="android:layout_marginRight">13dp</item>
    </style>

    <!--正餐菜品删除和打印条样式-->
    <style name="DishOperateButtonStyle" parent="PropertyActionButtonStyle">
        <item name="android:textSize">@dimen/text_12</item>
        <item name="android:drawablePadding">0dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/text_pay_other_black</item>
        <item name="android:background">@drawable/btn_selector</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">13dp</item>
        <item name="android:layout_marginRight">13dp</item>
    </style>

    <!--订单属性动作栏位-->

    <!--自定义键盘子项风格-->

    <style name="DefaultAppTheme" parent="@android:style/Theme.NoTitleBar.Fullscreen">

    </style>

</resources>