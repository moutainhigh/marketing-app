<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_39b2bf" />
            <corners android:radius="@dimen/btn_radius" />
            <stroke android:color="@color/color_39b2bf" android:width="0.5dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_dfe4e9" />
            <corners android:radius="@dimen/btn_radius" />
            <stroke android:color="@color/color_dfe4e9" android:width="0.5dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_40D0AE" />
            <corners android:radius="@dimen/btn_radius" />
            <stroke android:color="@color/color_40D0AE" android:width="0.5dp" />
        </shape>
    </item>
</selector>