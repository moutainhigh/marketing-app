<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#FACFB9" />
            <stroke android:width="1dp" android:color="#E4713D" />
            <corners android:radius="16.5dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFE6D9" />
            <stroke android:width="1dp" android:color="#FF8249" />
            <corners android:radius="16.5dip" />
        </shape>
    </item>
</selector>