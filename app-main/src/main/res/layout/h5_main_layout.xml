<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:focusableInTouchMode="true"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/h5_nav_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_bar_height"
        android:background="@drawable/cashier_bg_title_bar"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/h5_nav_back_button"
            style="@style/title_icon_style"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:minWidth="70dp"
            android:scaleType="centerInside"
            android:src="@drawable/customer_back_icon" />


        <FrameLayout
            android:id="@+id/h5_nav_right_layout"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/dinner_table_info_button_selector"
            android:minWidth="70dp"
            android:visibility="gone">

            <include layout="@layout/h5_main_layout_nav_dot_right" />
        </FrameLayout>

        <TextView
            android:id="@+id/h5_nav_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:textColor="#ffffff"
            android:textSize="18sp"
            tools:text="TextView" />

    </RelativeLayout>

    <com.zhongmei.bty.thirdplatform.astore.H5WebView
        android:id="@+id/h5_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>