                        style="@style/settings_left2"
                        android:tag="@string/front_silver"
                        android:text="@string/layout_option" />

                    <View
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/layout_item_settings"
                        style="@style/settings_left2"
                        android:tag="@string/front_silver"
                        android:text="@string/layout_item_settings" />

                    <View
                        android:id="@+id/v_snack_order_accept"
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:visibility="gone"
                        android:background="@color/print_line" />

                    <TextView
                        android:id="@+id/tv_snack_order_accept"
                        style="@style/settings_left2"
                        android:tag="@string/front_silver"
                        android:text="@string/order_accept_settings"
                        android:visibility="gone" />

                    <View
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/Seting_Document"
                        style="@style/settings_left2"
                        android:tag="@string/front_silver"
                        android:text="@string/setting_serial" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:id="@+id/tv_setting_order_display_device"
                        style="@style/settings_left2"
                        android:visibility="gone"
                        android:tag="@string/front_silver"
                        android:text="@string/setting_order_display_device" />

                    <TextView
                        android:visibility="gone"
                        style="@style/settings_left1"
                        android:text="@string/booking_setting" />

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/book_switch"
                        style="@style/settings_left2"
                        android:tag="@string/queue_setting"
                        android:text="@string/common_switch" />

                    <TextView
                        android:visibility="gone"
                        style="@style/settings_left1"
                        android:text="@string/queue_setting" />

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/queue_common_switch"
                        style="@style/settings_left2"
                        android:tag="@string/queue_setting"
