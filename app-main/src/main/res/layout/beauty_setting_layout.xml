<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/dinnerdish_bg">

    <fragment
        android:id="@+id/tbf_status_bar"
        android:name="com.zhongmei.bty.dinner.orderdish.TitleBarFragment_"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:baselineAligned="false"
        android:orientation="horizontal">

        <LinearLayout
            style="@style/settings_leftTitlell"
            android:layout_weight="1"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/title_bar_height"
                android:background="@color/settings_blue"
                android:paddingRight="3dp">

                <TextView
                    android:id="@+id/setting_title"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/title_bar_height"
                    android:gravity="center"
                    android:text="@string/set"
                    android:textColor="#FFFFFF"
                    android:textSize="22sp" />
            </FrameLayout>

            <ScrollView
                android:id="@+id/sv_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/dinner_left_bg_shadow"
                android:gravity="center">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        style="@style/settings_left1"
                        android:text="@string/peripheral_management" />


                    <TextView
                        android:id="@+id/Seting_printer"
                        style="@style/settings_left2"
                        android:tag="@string/peripheral_management"
                        android:text="@string/printer" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:id="@+id/Seting_moneyBox"
                        style="@style/settings_left2"
                        android:tag="@string/peripheral_management"
                        android:text="@string/cashbox" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:id="@+id/Seting_scanner"
                        style="@style/settings_left2"
                        android:tag="@string/peripheral_management"
                        android:text="@string/scanner" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:id="@+id/Seting_blue_phone"
                        style="@style/settings_left2"
                        android:tag="@string/peripheral_management"
                        android:text="@string/setting_phone" />

                    <TextView
                        style="@style/settings_left1"
                        android:text="@string/setting_pay_and_hand_over_set_label" />

                    <TextView
                        android:id="@+id/Setting_PayModelOrder"
                        style="@style/settings_left2"
                        android:tag="@string/setting_pay_and_hand_over_set_label"
                        android:text="@string/setting_pay_method_set_label" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/Setting_GroupPay"
                        style="@style/settings_left2"
                        android:tag="@string/setting_pay_and_hand_over_set_label"
                        android:text="@string/setting_group_pay_setting_label"
                        android:visibility="gone" />

                    <View
                        android:id="@+id/v_snack_handover_type"
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:id="@+id/tv_snack_handover_type"
                        style="@style/settings_left2"
                        android:tag="@string/setting_pay_and_hand_over_set_label"
                        android:text="@string/handover_type_setting"
                        android:visibility="gone" />

                    <TextView
                        style="@style/settings_left1"
                        android:text="@string/print_allocation" />

                    <TextView
                        android:id="@+id/setting_basic"
                        style="@style/settings_left2"
                        android:tag="@string/print_allocation"
                        android:visibility="gone"
                        android:text="@string/print_basic_setting" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:id="@+id/Setting_CashierPointManagement"
                        style="@style/settings_left2"
                        android:tag="@string/print_allocation"
                        android:text="@string/cash_point_management" />

                    <View
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/Setting_KitchenManagement"
                        style="@style/settings_left2"
                        android:tag="@string/print_allocation"
                        android:text="@string/kitchen_management" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />


                    <TextView
