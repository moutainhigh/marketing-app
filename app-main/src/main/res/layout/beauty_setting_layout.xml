                    <View
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/queue_manager"
                        style="@style/settings_left2"
                        android:tag="@string/queue_setting"
                        android:text="@string/queue_manager" />

                    <View
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/voice_manager"
                        style="@style/settings_left2"
                        android:tag="@string/queue_setting"
                        android:text="@string/voice_manager" />


                    <TextView
                        style="@style/settings_left1"
                        android:text="@string/notify_center" />

                    <TextView
                        android:id="@+id/Seting_notifity"
                        style="@style/settings_left2"
                        android:tag="@string/notify_center"
                        android:text="@string/notify_nofity" />


                    <TextView
                        style="@style/settings_left1"
                        android:text="@string/system_information" />

                    <TextView
                        android:id="@+id/Seting_customerInfo"
                        style="@style/settings_left2"
                        android:tag="@string/system_information"
                        android:text="@string/merchant_information" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/Seting_verionInfo"
                            style="@style/settings_left2"
                            android:tag="@string/system_information"
                            android:text="@string/version_information" />

                        <ImageView
                            android:id="@+id/iv_update_remind"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="54dp"
                            android:src="@drawable/booking_date_prompt" />
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="@color/print_line" />

                    <TextView
                        android:id="@+id/Seting_systemJournal"
                        style="@style/settings_left2"
                        android:tag="@string/system_information"
                        android:text="@string/system_log" />
                </LinearLayout>
            </ScrollView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/right_container1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2.3"
            android:background="#ffffff"
            android:orientation="vertical"></LinearLayout>
        <!--android:background="@drawable/settings_fragment_background"-->

    </LinearLayout>
</LinearLayout>
